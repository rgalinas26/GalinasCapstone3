@model IList<Park>
@{
    ViewData["Title"] = "Home Page";
}



<table class="table table-bordered">

    @foreach (Park park in Model)
    {
        <tr>
            <td>
                <img src="~/images/parks/@(park.Park_Code).jpg" />
            </td>
            <td>
                <b>Location:</b> <br /> @park.State
            </td>
            <td>
                <a asp-controller="home" asp-action="Detail" asp-route-parkCode="@park.Park_Code">
                    <b>Park Name: </b><br />@park.Park_Name
                </a>
            </td>
            <td>
                <b>Description:</b><br />@park.Description
            </td>

        </tr>
    }
</table>
